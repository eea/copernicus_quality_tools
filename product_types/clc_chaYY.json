{
  "description": "CORINE Land Cover change layer",
  "parameters": {
    "layer_prefix": "^countrycode/cha",
    "layer_regex": "^countrycode/cha[0-9]{2}_countrycode$",
    "layer_count": 1,
    "product_code": "cha"

  },
  "checks": [
    {
      "check_ident": "v1",
      "required": true,
      "parameters": {
        "formats": [".gdb"]
      }
    },
    {
      "check_ident": "v2",
      "required": false,
      "parameters": {
        "file_name_regex": "^clc[0-9]{4}_countrycode.gdb$"
      }
    },
    {
      "check_ident": "v3",
      "required": false,
      "parameters": {
        "fields": ["^ID$", "^CODE_[0-9]{2}$", "^CODE_[0-9]{2}$", "^CHANGE$", "^CHTYPE$", "^AREA_HA$", "^REMARK$", "^Shape_Length$", "^Shape_Area$"]
      }
    },
    {
      "check_ident": "v4",
      "required": false,
      "parameters": {
        "epsg": [3035]
      }
    },
    {
      "check_ident": "import2pg",
      "required": true
    },
    {
      "check_ident": "v5",
      "required": false
    },
    {
      "check_ident": "v6",
      "required": false
    },
    {
      "check_ident": "v11",
      "required": false,
      "parameters": {
        "area_ha": 5,
        "border_exception": true
      }
    }
  ]
}
