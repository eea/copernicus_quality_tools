{
  "description": "CORINE Land Cover status layers",
  "parameters": {
    "layer_prefix": "^countrycode/clc",
    "layer_regex": "^countrycode/clc[0-9]{2}_countrycode$",
    "layer_count": 2
  },
  "checks": [
    {
      "check_ident": "v1",
      "required": true,
      "parameters": {
          "formats": [".gdb"]
      }
    },
    {
      "check_ident": "v2",
      "required": false,
      "parameters": {
        "file_name_regex": "^clc[0-9]{4}_countrycode.gdb$"
      }
    },
    {
      "check_ident": "v3",
      "required": false,
      "parameters": {
        "fields": ["^ID$", "^CODE_[0-9]{2}$", "^AREA_HA$", "^REMARK$", "^Shape_Length$", "^Shape_Area$"]
      }
    },
    {
      "check_ident": "v4",
      "required": false,
      "parameters": {
          "epsg": [3035, 23033, 5514]
      }
    },
    {
      "check_ident": "import2pg",
      "required": true
    },
    {
      "check_ident": "v11",
      "required": false,
      "parameters": {
        "area_ha": 200,
        "border_exception": true
      }
    }
  ]
}
