{
  "description": "CORINE Land Cover initial status layer",
  "parameters": {
    "layer_prefix": "^countrycode/clc",
    "layer_regex": "^countrycode/clc[0-9]{2}_countrycode$",
    "layer_count": 2
  },
  "checks": [
    {
      "check_ident": "v1",
      "required": true,
      "parameters": {
          "formats": [".gdb"]
      }
    },
    {
      "check_ident": "v2",
      "required": false,
      "parameters": {
        "file_name_regex": "^clc[0-9]{4}_countrycode.gdb$"
      }
    },
    {
      "check_ident": "v4",
      "required": false,
      "parameters": {
          "epsg": [3035, 23033]
      }
    },
    {
      "check_ident": "v11",
      "required": false,
      "parameters": {
          "area_ha": 25
      }
    }
  ]
}
