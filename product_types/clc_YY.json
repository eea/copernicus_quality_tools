{
  "description": "CORINE Land Cover initial status layer",
  "parameters": {
    "layer_prefix": "clc",
    "layer_regex": "clc[0-9]{2}_[a-z]{2}",
    "layer_count": 2
  },
  "checks": [
    {
      "check_ident": "v1",
      "required": true,
      "parameters": {
          "formats": [".gdb"]
      }
    },
    {
      "check_ident": "v2",
      "required": false,
      "parameters": {
          "file_name_regex": "^clc[0-9][0-9][0-9][0-9]_[a-z][a-z].gdb$",
          "feature_dataset_regex": "^[a-z][a-z]$",
          "feature_class_regex": "^clc[0-9][0-9]_[a-z][a-z]$"
      }
    },
    {
      "check_ident": "v4",
      "required": false,
      "parameters": {
          "epsg": [3035]
      }
    },
    {
      "check_ident": "v11",
      "required": false,
      "parameters": {
          "area_ha": 25
      }
    }
  ]
}
