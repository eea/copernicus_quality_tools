{
  "description": "HRL Tree cover density (20m)",
  "steps": [
    {
      "check_ident": "qc_tool.raster.unzip",
      "required": true
    },
    {
      "check_ident": "qc_tool.raster.format",
      "required": true,
      "parameters": {
        "formats": [".tif"],
        "drivers": {".tif": "GTiff"}
      }
    },
    {
      "check_ident": "qc_tool.raster.naming",
      "required": true,
      "parameters": {
        "file_name_regex": "^tcd_(?P<reference_year>[0-9]{4})_020m_(?P<country_code>.+)_[0-9]{5}.*.tif$",
        "extensions": [".tif", ".tfw", ".xml|.tif.xml", ".tif.vat.dbf"],
        "country_codes": ["al", "at",
                          "ba", "be", "bg",
                          "ch", "cy", "cz",
                          "de", "dk",
                          "ee", "es", "es_cn", "eu",
                          "fi", "fr", "fr_glp", "fr_guf", "fr_mtq", "fr_myt", "fr_reu",
                          "gr",
                          "hr", "hu",
                          "ie", "is", "it",
                          "li", "lt", "lu", "lv",
                          "me", "mk", "mt",
                          "nl", "no",
                          "pl", "pt", "pt_raa", "pt_ram", "pt_raa_ceg", "pt_raa_weg",
                          "ro",
                          "se", "si", "sk",
                          "tr",
                          "uk", "uk_ni", "uk_ge", "uk_je",
                          "xk"]
      }
    },
    {
      "check_ident": "qc_tool.raster.attribute",
      "required": false,
      "parameters": {
        "attribute_regexes": ["value", "count", "(km2|area_km2)", "(perc|area_perc)", "(class_name|classnames)"]
      }
    },
    {
      "check_ident": "qc_tool.raster.epsg",
      "required": false,
      "parameters": {
        "epsg": [3035]
      }
    },
    {
      "check_ident": "qc_tool.raster.pixel_size",
      "required": false,
      "parameters": {
        "pixelsize": 20
      }
    },
    {
      "check_ident": "qc_tool.raster.origin",
      "required": false,
      "parameters": {
      }
    },
    {
      "check_ident": "qc_tool.raster.bit_depth",
      "required": false,
      "parameters": {
        "datatype": "byte"
      }
    },
    {
      "check_ident": "qc_tool.raster.compress",
      "required": false,
      "parameters": {
        "compression": ["LZW", "PackBits"]
