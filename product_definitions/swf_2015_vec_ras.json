{
  "description": "HRL Small Woody Features, vector + 5m raster",
  "steps": [
    {
      "check_ident": "qc_tool.raster.unzip",
      "required": true
    },
    {
      "check_ident": "qc_tool.raster.naming",
      "required": true,
      "parameters": {
        "layer_names":{"swf_raster": "swf_2015_005m_[a-z_]+_0?3035_(?P<aoi_code>[0-9]+)_"},
        "extensions": [".tif"]
      }
    },
    {
      "check_ident": "qc_tool.raster.format",
      "required": true,
      "parameters": {
        "layers": ["swf_raster"],
        "formats": [".tif"],
        "drivers": {".tif": "GTiff"}
      }
    },
    {
      "check_ident": "qc_tool.raster.epsg",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "auto_identify_epsg": true,
        "epsg": 3035
      }
    },
    {
      "check_ident": "qc_tool.raster.pixel_size",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "pixelsize": 5
      }
    },
    {
      "check_ident": "qc_tool.raster.origin",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"]
      }
    },
    {
      "check_ident": "qc_tool.raster.bit_depth",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "datatype": "byte"
      }
    },
    {
      "check_ident": "qc_tool.raster.compress",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "compression": ["LZW"]
      }
    },
    {
      "check_ident": "qc_tool.raster.value",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "validcodes": [0, 1, 2, 3, 254, 255]
      }
    },
    {
      "check_ident": "qc_tool.raster.color",
      "required": false,
      "parameters": {
        "layers": ["swf_raster"],
        "colors": {
          "0":[240, 240, 240],
          "1":[215, 245, 0],
          "3":[240, 200, 0],
          "254":[153, 153, 153],
          "255":[0, 0, 0]
        }
      }
    },
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.naming",
      "required": true,
      "parameters": {
        "reference_year": "2015",
        "formats": [
          ".gdb",
          ".shp"
        ],
        "layer_names": {
          "swf_vec": "swf_2015_vec_[a-z_]+_0?3035_(?P<aoi_code>[0-9]+)_"
        }
      }
    },
    {
      "check_ident": "qc_tool.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["swf_vec"],
        "attributes": {"code": "string",
                       "area": "real",
                       "class_name": "string"}
      }
    },
    {
      "check_ident": "qc_tool.vector.epsg",
      "required": true,
      "parameters": {
        "layers": ["swf_vec"],
        "epsg": 3035,
        "auto_identify_epsg": true
      }
    },
    {
      "check_ident": "qc_tool.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "column_defs": [["code", ["1", "2", "3"]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.singlepart",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"]
      }
    },
    {
      "check_ident": "qc_tool.vector.geometry",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"]
      }
    },
    {
      "check_ident": "qc_tool.vector.layer_area",
      "required": false,
      "parameters": {
        "layers": ["swf_raster", "swf_vec"],
        "vector_layer": "swf_vec",
        "vector_code_column_name": "code",
        "vector_codes": ["1", "2", "3"],
        "raster_layer": "swf_raster",
        "raster_codes": [1, 3]
      }
    },
    {
      "check_ident": "qc_tool.vector.compactness",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "area_column_name": "area",
        "linear_code": "1",
        "patchy_code": "2",
        "threshold": 0.75
      }
    },
    {
      "check_ident": "qc_tool.vector.mmu",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "filter_code": "2",
        "area_column_name": "area",
        "mmu": 200
      }
    },
    {
      "check_ident": "qc_tool.vector.mxmu",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "filter_code": "2",
        "area_column_name": "area",
        "mxmu": 5000
      }
    },
    {
      "check_ident": "qc_tool.vector.mmw",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "filter_code": "2",
        "mmw": 10
      }
    },
    {
      "check_ident": "qc_tool.vector.mxmw",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "filter_code": "1",
        "mxmw": 30
      }
    },
    {
      "check_ident": "qc_tool.vector.mml",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_name": "code",
        "filter_code": "1",
        "mml": 50
      }
    },
    {
      "check_ident": "qc_tool.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"]
      }
    },
    {
      "check_ident": "qc_tool.vector.neighbour",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"],
        "code_column_names": ["code"]
      }
    },
    {
      "check_ident": "qc_tool.vector.inspire",
      "required": false,
      "parameters": {
        "layers": ["swf_vec"]
      }
    }
  ]
}
