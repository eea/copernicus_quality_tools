{
  "description": "Urban Atlas 2012, geodatabase, all layers included.",
  "checks": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "v1_ua_gdb",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "reference_layer_regex": "_ua2012$",
        "boundary_layer_regex": "^boundary2012_",
        "revised_layer_regex": "_ua2006_revised$",
        "combined_layer_regex": "_ua2006_2012$",
        "change_layer_regex": "_change_2006_2012$"
      }
    },
    {
      "check_ident": "v2",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "formats": [".gdb"]
      }
    },
    {
      "check_ident": "reference.v3",
      "required": true,
      "parameters": {
        "layers": ["reference"],
          "attribute_regexes": ["country", "cities", "fua_or_cit", "code2012", "item2012", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "boundary.v3",
      "required": true,
      "parameters": {
        "layers": ["boundary"],
        "attribute_regexes": ["country", "cities"]
      }
    },
    {
      "check_ident": "revised.v3",
      "required": true,
      "parameters": {
        "layers": ["revised"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code2006", "item2006", "prod_date", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "combined.v3",
      "required": true,
      "parameters": {
        "layers": ["combined", "change"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code2006", "item2006", "code2012", "item2012", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "v4",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "epsg": [3035]
      }
    },
    {
      "check_ident": "v_import2pg",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "v5",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "change"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "reference.v6",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code2012", "UA"]]
      }
    },
    {
      "check_ident": "revised.v6",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "column_defs": [["code2006", "UA"]]
      }
    },
    {
      "check_ident": "combined.v6",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "column_defs": [["code2006", "UA"], ["code2012", "UA"]]
      }
    },
    {
      "check_ident": "v8",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "reference.v11_ua_status",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "area_column_name": "shape_area",
        "code_column_name": "code2012"
      }
    },
    {
      "check_ident": "revised.v11_ua_status",
      "required": false,
      "parameters": {
        "layers": ["revised", "combined"],
        "area_column_name": "shape_area",
        "code_column_name": "code2006"
      }
    },
    {
      "check_ident": "change.v11_ua_change",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "area_column_name": "shape_area",
        "initial_code_column_name": "code2006",
        "final_code_column_name": "code2012"
      }
    },
    {
      "check_ident": "v13",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "reference.v14",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_names": ["code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "revised.v14",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_column_names": ["code2006"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "change.v14",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "code_column_names": ["code2006", "code2012"],
        "exclude_codes": ["122%"]
      }
    }
  ]
}
