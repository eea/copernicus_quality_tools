{
  "description": "Urban Atlas 2012, geodatabase, all layers included.",
  "checks": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "v1_ua_gdb",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "gdb_filename_regex": "^[a-z0-9]{7}_.*$",
        "reference_layer_regex": "_ua2012$",
        "boundary_layer_regex": "^boundary2012_",
        "revised_layer_regex": "_ua2006_revised$",
        "combined_layer_regex": "_ua2006_2012$",
        "change_layer_regex": "_change_2006_2012$"
      }
    },
    {
      "check_ident": "v2",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "formats": [".gdb"]
      }
    },
    {
      "check_ident": "reference.v3",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_length": "real",
                       "shape_area": "real",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "boundary.v3",
      "required": true,
      "parameters": {
        "layers": ["boundary"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "shape_length": "real",
                       "shape_area": "real"}
      }
    },
    {
      "check_ident": "revised.v3",
      "required": true,
      "parameters": {
        "layers": ["revised"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_length": "real",
                       "shape_area": "real",
                       "code2006": "string",
                       "item2006": "string"}
      }
    },
    {
      "check_ident": "combined_change.v3",
      "required": true,
      "parameters": {
        "layers": ["combined", "change"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_length": "real",
                       "shape_area": "real",
                       "code2006": "string",
                       "item2006": "string",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "v4",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "epsg": [3035]
      }
    },
    {
      "check_ident": "v_import2pg",
      "required": true
    },
    {
      "check_ident": "v5",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "change"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "reference.v6",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code2012", "UA"]]
      }
    },
    {
      "check_ident": "revised.v6",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "column_defs": [["code2006", "UA2006"]]
      }
    },
    {
      "check_ident": "combined_change.v6",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "column_defs": [["code2006", "UA2006"], ["code2012", "UA"]]
      }
    },
    {
      "check_ident": "v8",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "v10",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised"]
      }
    },
    {
      "check_ident": "reference.v11_ua_status",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "area_column_name": "shape_area",
        "code_column_name": "code2012"
      }
    },
    {
      "check_ident": "revised.v11_ua_status",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "area_column_name": "shape_area",
        "code_column_name": "code2006"
      }
    },
    {
      "check_ident": "change.v11_ua_change",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "area_column_name": "shape_area",
        "initial_code_column_name": "code2006",
        "final_code_column_name": "code2012"
      }
    },
    {
      "check_ident": "reference.v12_ua",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_name": "code2012",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "revised.v12_ua",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_column_name": "code2006",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "v13",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "reference.v14",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_names": ["code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "revised.v14",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_column_names": ["code2006"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "combined_change.v14",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "code_column_names": ["code2006", "code2012"],
        "exclude_codes": ["122%"]
      }
    }
  ]
}
