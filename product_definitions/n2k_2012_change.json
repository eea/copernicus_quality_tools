                     "code_4_12": "integer",
                     "nodata_12": "integer",
                     "comment": "string",
                     "changecode": "string",
                     "area_ha": "real"},
        "ignored": ["shape_area", "shape_length"]
      }
    },
    {
      "check_ident": "qc_tool.vector.epsg",
      "required": true,
      "parameters": {
        "layers": ["change2012"],
        "epsg": 3035,
        "auto_identify_epsg": false
      }
    },
    {
