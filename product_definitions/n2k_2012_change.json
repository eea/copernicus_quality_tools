    {
      "check_ident": "qc_tool.vector.naming",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "formats": [".gdb", ".gpkg"],
        "layer_names": {
          "change2012": "^n2k_(?P<delivery_unit_id>du[0-9]{3}[a-z])_Change2006_2012_lclu_v[0-9]+_[0-9]{8}$"
