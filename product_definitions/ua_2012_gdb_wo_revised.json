{
  "description": "Urban Atlas 2012, geodatabase, without revised layer.",
  "steps": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "v1_ua_gdb",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "gdb_filename_regex": "^[a-z0-9]{7}_.*$",
        "reference_layer_regex": "_ua2012$",
        "boundary_layer_regex": "^boundary2012_"
      }
    },
    {
      "check_ident": "v2",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary"],
        "formats": [".gdb"],
        "drivers": {".gdb": "OpenFileGDB"}
      }
    },
    {
      "check_ident": "reference.v3",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_length": "real",
                       "shape_area": "real",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "v4",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary"],
        "epsg": [3035]
      }
    },
    {
      "check_ident": "v_import2pg",
      "required": true
    },
    {
      "check_ident": "v5",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "v6",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code2012", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "21000", "22000", "23000", "24000", "25000",
                                      "31000", "32000", "33000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]]]
      }
    },
    {
      "check_ident": "v8",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "v9",
      "required": false,
      "parameters": {
        "layers": ["boundary", "reference"]
      }
    },
    {
      "check_ident": "v10",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "v11_ua_status",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "area_column_name": "shape_area",
        "code_column_name": "code2012"
      }
    },
    {
      "check_ident": "v12_ua",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_name": "code2012",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "v13",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "v14",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_names": ["code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "v15",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    }
  ]
}
