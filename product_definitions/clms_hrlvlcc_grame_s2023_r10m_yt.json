{
  "description": "HRL Grassland mowing events 2023 (10m): DOM-YT",
  "steps": [
    {
      "check_ident": "qc_tool.raster.unzip",
      "required": 1
    },
    {
      "check_ident": "qc_tool.raster.naming",
      "required": 1,
      "parameters": {
        "layer_names": {
          "raster": "^clms_hrlvlcc_grame_s2023_r10m_(yt|YT)_(?P<epsg_code>[0-9]{5})_v[0-9]{2}_r[0-9]{2}"
        },
        "epsg_codes": [
          "32738"
        ],
        "extensions": [
          ".tif"
        ],
        "reference_year": "2023"
      }
    },
    {
      "check_ident": "qc_tool.raster.inspire",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ]
      }
    },
    {
      "check_ident": "qc_tool.raster.attribute",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "attribute_regexes": [
          "value",
          "count",
          "area_km2",
          "area_perc",
          "class_name"
        ]
      }
    },
    {
      "check_ident": "qc_tool.raster.epsg",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "epsg": 32738
      }
    },
    {
      "check_ident": "qc_tool.raster.pixel_size",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "pixelsize": 10
      }
    },
    {
      "check_ident": "qc_tool.raster.bit_depth",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "datatype": "byte"
      }
    },
    {
      "check_ident": "qc_tool.raster.compress",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "compression": [
          "LZW"
        ]
      }
    },
    {
      "check_ident": "qc_tool.raster.value",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "validcodes": [
          0,
          1,
          2,
          3,
          4,
          255
        ]
      }
    },
    {
      "check_ident": "qc_tool.raster.tile",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "max_blocksize": 1024
      }
    },
    {
      "check_ident": "qc_tool.raster.mmu",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "area_pixels": 25,
        "value_exception_codes": [
          255
        ],
        "neighbour_exception_codes": [
          255
        ],
        "nodata_value": 255,
        "check_neighbours": 0,
        "boundary_source": "boundary_eea39.gpkg"
      }
    },
    {
      "check_ident": "qc_tool.raster.color",
      "required": 0,
      "parameters": {
        "layers": [
          "raster"
        ],
        "colors": {
          "0": [
            240,
            240,
            240
          ],
          "1": [
            237,
            248,
            251
          ],
          "2": [
            178,
            226,
            226
          ],
          "3": [
            102,
            194,
            164
          ],
          "4": [
            35,
            139,
            69
          ],
          "255": [
            0,
            0,
            0
          ]
        }
      }
    }
  ]
}