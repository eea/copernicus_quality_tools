{
  "description": "Urban Atlas 2012, shapefile, without revised layer.",
  "steps": [
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.naming_ua_shp",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "reference_layer_regex": "_ua2012$",
        "boundary_layer_regex": "^boundary2012_"
      }
    },
    {
      "check_ident": "qc_tool.vector.format",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary"],
        "formats": [".shp"],
        "drivers": {".shp": "ESRI Shapefile"}
      }
    },
    {
      "check_ident": "qc_tool.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_leng": "real",
                       "shape_area": "real",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "qc_tool.vector.epsg",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary"],
        "epsg": [3035],
        "auto_identify_epsg": true
      }
    },
    {
      "check_ident": "qc_tool.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.unique",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code2012", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "21000", "22000", "23000", "24000", "25000",
                                      "31000", "32000", "33000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.singlepart",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "qc_tool.vector.geometry",
      "required": false,
      "parameters": {
        "layers": ["boundary", "reference"]
      }
    },
    {
      "check_ident": "qc_tool.vector.gap",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "qc_tool.vector.mmu_ua_status",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "area_column_name": "shape_area",
        "code_column_name": "code2012"
      }
    },
    {
      "check_ident": "qc_tool.vector.mmw_ua",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_name": "code2012",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "qc_tool.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    },
    {
      "check_ident": "qc_tool.vector.neighbour",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_names": ["code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "qc_tool.vector.inspire",
      "required": false,
      "parameters": {
        "layers": ["reference"]
      }
    }
  ]
}
