{
  "description": "Urban Atlas with Change Layer",
  "checks": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "boundary.v1_ua",
      "required": true,
      "parameters": {
        "layer_regex": "boundary(?P<reference_year>2006|2012|2018)_",
        "layer_count": 1,
        "is_border_source": true
      }
    },
    {
      "check_ident": "boundary.v2",
      "required": true,
      "parameters": {
        "formats": [".gdb", ".shp"]
      }
    },
    {
      "check_ident": "boundary.v3",
      "required": true,
      "parameters": {
        "attribute_regexes": ["country", "cities"]
      }
    },
    {
      "check_ident": "boundary.v4",
      "required": true,
      "parameters": {
        "epsg": [3035]
      }
    },
    {
      "check_ident": "boundary.v_import2pg",
      "required": true
    },
    {
      "check_ident": "status.v1_ua",
      "required": true,
      "parameters": {
        "layer_regex": ".*_ua(?P<reference_year>2006_2012|2012|2018)$",
        "layer_count": 2,
        "is_border_source": false
      }
    },
    {
      "check_ident": "status.v2",
      "required": true,
      "parameters": {
        "formats": [".gdb", ".shp"]
      }
    },
    {
      "check_ident": "status.v3",
      "required": true,
      "parameters": {
        "attribute_regexes": ["country", "cities", "(fua|luz)_or_cit", "code(2006|2012|2018)", "item(2006|2012|2018)", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "status.v4",
      "required": true,
      "parameters": {
        "epsg": [3035]
      }
    },
    {
      "check_ident": "status.v_import2pg",
      "required": true
    },
    {
      "check_ident": "status.v5",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "status.v6",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{6})$",
        "code_to_column_defs": {"ua2006": [["code2006", "UA"]],
                                "ua2012": [["code2012", "UA"]],
                                "ua2018": [["code2018", "UA"]]}
      }
    },
    {
      "check_ident": "status.v8",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "status.v11_ua",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{6})$",
        "code_to_column_name": {"ua2006": "code2006",
                                "ua2012": "code2012",
                                "ua2018": "code2018"}

      }
    },
    {
      "check_ident": "status.v13",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "status.v14",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{6})$",
        "code_to_column_names": {"ua2006": ["code2006"],
                                 "ua2012": ["code2012"],
                                 "ua2018": ["code2018"]}
      }
    },
    {
      "check_ident": "change.v1_ua",
      "required": true,
      "parameters": {
        "layer_regex": ".*_change_(?P<reference_year1>2006|2012)_(?P<reference_year>2012|2018)$",
        "layer_count": 1,
        "is_border_source": false
      }
    },
    {
      "check_ident": "change.v2",
      "required": true,
      "parameters": {
        "formats": [".gdb", ".shp"]
      }
    },
    {
      "check_ident": "change.v3",
      "required": false,
      "parameters": {
        "attribute_regexes": ["objectid", "country", "cities", "(fua|luz)_or_cit", "code(2006|2012)", "code(2012|2018)", "item(2006|2012)", "item(2012|2018)", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "change.v4",
      "required": false,
      "parameters": {
        "epsg": [3035]
      }
    },
    {
      "check_ident": "change.v_import2pg",
      "required": true
    },
    {
      "check_ident": "change.v5",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "change.v6",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{16})$",
        "code_to_column_defs": {"change_2006_2012": [["code2006", "UA"], ["code2012", "UA"]],
                                "change_2012_2018": [["code2012", "UA"], ["code2018", "UA"]]}
      }
    },
    {
      "check_ident": "change.v8",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "change.v11_ua",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{16})$",
        "code_to_column_name": {"change_2006_2012": "code2012",
                                "change_2012_2018": "code2018"}
      }
    },
    {
      "check_ident": "change.v13",
      "required": false,
      "parameters": {
        "ident_colname": "ident"
      }
    },
    {
      "check_ident": "change.v14",
      "required": false,
      "parameters": {
        "ident_colname": "ident",
        "code_regex": "(.{16})$",
        "code_to_column_names": {"change_2006_2012": ["code2006", "code2012"],
                                 "change_2012_2018": ["code2012", "code2018"]}
      }
    }
  ]
}
