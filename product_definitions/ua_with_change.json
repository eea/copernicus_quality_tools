{
  "description": "Urban Atlas with Change Layer",
  "checks": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "v1_ua",
      "required": true,
      "parameters": {
        "campaign_years": ["2006", "2012", "2018"],
        "reference_layer_regex": "_ua(?P<reference_year>[0-9]{{4}})$",
        "boundary_layer_regex": "^boundary{reference_year:s}_",
        "revised_layer_regex": "_ua{revised_year:s}_revised$",
        "combined_layer_regex": "_ua{revised_year:s}_{reference_year:s}$",
        "change_layer_regex": "_change_{revised_year:s}_{reference_year:s}$"
      }
    },
    {
      "check_ident": "v2",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "formats": [".gdb", ".shp"]
      }
    },
    {
      "check_ident": "reference.v3",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code(2012|2018)", "item(2012|2018)", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "boundary.v3",
      "required": true,
      "parameters": {
        "layers": ["boundary"],
        "attribute_regexes": ["country", "cities"]
      }
    },
    {
      "check_ident": "combined.v3",
      "required": true,
      "parameters": {
        "layers": ["combined"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code2006", "item2006", "code2012", "item2012", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "revised.v3",
      "required": true,
      "parameters": {
        "layers": ["revised"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code2006", "item2006", "prod_date", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "change.v3",
      "required": true,
      "parameters": {
        "layers": ["change"],
        "attribute_regexes": ["country", "cities", "fua_or_cit", "code(2006|2012)", "code(2012|2018)", "item(2006|2012)", "item(2012|2018)", "prod_date", "ident", "shape_length", "shape_area"]
      }
    },
    {
      "check_ident": "v4",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "epsg": [3035]
      }
    },
    {
      "check_ident": "v_import2pg",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "v5",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "change"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "reference.v6",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_regex": "(ua2012|ua2018)",
        "code_to_column_defs": {"ua2012": [["code2012", "UA"]],
                                "ua2018": [["code2018", "UA"]]}
      }
    },
    {
      "check_ident": "combined.v6",
      "required": false,
      "parameters": {
        "layers": ["combined"],
        "code_regex": "(ua2006)",
        "code_to_column_defs": {"ua2006": [["code2006", "UA"], ["code2012", "UA"]]}
      }
    },
    {
      "check_ident": "revised.v6",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_regex": "(ua2006)",
        "code_to_column_defs": {"ua2006": [["code2006", "UA"]]}
      }
    },
    {
      "check_ident": "change.v6",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "code_regex": "(.{16})$",
        "code_to_column_defs": {"change_2006_2012": [["code2006", "UA"], ["code2012", "UA"]],
                                "change_2012_2018": [["code2012", "UA"], ["code2018", "UA"]]}
      }
    },
    {
      "check_ident": "v8",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "reference.v11_ua",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_regex": "(ua2012|ua2018)",
        "code_to_column_name": {"ua2012": "code2012",
                                "ua2018": "code2018"}

      }
    },
    {
      "check_ident": "combined.v11_ua",
      "required": false,
      "parameters": {
        "layers": ["combined"],
        "code_regex": "(ua2006)",
        "code_to_column_name": {"ua2006": "code2006"}

      }
    },
    {
      "check_ident": "revised.v11_ua",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_regex": "(ua2006)",
        "code_to_column_name": {"ua2006": "code2006"}

      }
    },
    {
      "check_ident": "change.v11_ua",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "code_regex": "(.{16})$",
        "code_to_column_name": {"change_2006_2012": "code2012",
                                "change_2012_2018": "code2018"}
      }
    },
    {
      "check_ident": "v13",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "reference.v14",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_regex": "(ua2012|ua2018)",
        "code_to_column_names": {"ua2012": ["code2012"],
                                 "ua2018": ["code2018"]}
      }
    },
    {
      "check_ident": "combined.v14",
      "required": false,
      "parameters": {
        "layers": ["combined"],
        "code_regex": "(ua2006)",
        "code_to_column_names": {"ua2006": ["code2006", "code2012"]}
      }
    },
    {
      "check_ident": "revised.v14",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_regex": "(ua2006)",
        "code_to_column_names": {"ua2006": ["code2006"]}
      }
    },
    {
      "check_ident": "change.v14",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "code_regex": "(.{16})$",
        "code_to_column_names": {"change_2006_2012": ["code2006", "code2012"],
                                 "change_2012_2018": ["code2012", "code2018"]},
        "exclude_codes": ["122%"]
      }
    }
  ]
}
