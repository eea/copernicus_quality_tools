{
  "description": "Urban Atlas 2012, shapefile, all layers included.",
  "steps": [
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.naming",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "formats": [".shp"],
        "layer_names": {
          "reference": "_ua2012$",
          "boundary": "^boundary2012_",
          "revised": "_ua2006_revised$",
          "combined": "_ua2006_2012$",
          "change": "_change_2006_2012$"
        }
      }
    },
    {
      "check_ident": "qc_tool.vector.format",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "formats": [".shp"],
        "drivers": {".shp": "ESRI Shapefile"}
      }
    },
    {
      "check_ident": "qc_tool.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "shape_leng": "real",
                       "shape_area": "real",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "qc_tool.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["revised"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "perimeter": "real",
                       "area": "real",
                       "code2006": "string",
                       "item2006": "string"}
      }
    },
    {
      "check_ident": "qc_tool.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["combined", "change"],
        "attributes": {"country": "string",
                       "cities": "string",
                       "fua_or_cit": "string",
                       "prod_date": "string",
                       "perimeter": "real",
                       "area": "real",
                       "code2006": "string",
                       "item2006": "string",
                       "code2012": "string",
                       "item2012": "string",
                       "ident": "string"}
      }
    },
    {
      "check_ident": "qc_tool.vector.epsg",
      "required": true,
      "parameters": {
        "layers": ["reference", "boundary", "combined", "revised", "change"],
        "epsg": [3035],
        "auto_identify_epsg": true
      }
    },
    {
      "check_ident": "qc_tool.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.unique",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "change"],
        "unique_keys": ["ident"]
      }
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code2012", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "21000", "22000", "23000", "24000", "25000",
                                      "31000", "32000", "33000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "column_defs": [["code2006", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "20000",
                                      "30000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "column_defs": [["code2006", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "20000",
                                      "30000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]],
                        ["code2012", ["11100", "11210", "11220", "11230", "11240", "11300",
                                      "12100", "12210", "12220", "12230", "12300", "12400",
                                      "13100", "13300", "13400",
                                      "14100", "14200",
                                      "21000", "22000", "23000", "24000", "25000",
                                      "31000", "32000", "33000",
                                      "40000",
                                      "50000",
                                      "91000", "92000"]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.singlepart",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "qc_tool.vector.gap",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised"]
      }
    },
    {
      "check_ident": "qc_tool.vector.mmu_ua_status",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "area_column_name": "shape_area",
        "code_column_name": "code2012"
      }
    },
    {
      "check_ident": "qc_tool.vector.mmu_ua_status",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "area_column_name": "shape_area",
        "code_column_name": "code2006"
      }
    },
    {
      "check_ident": "qc_tool.vector.mmu_ua_change",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "area_column_name": "shape_area",
        "initial_code_column_name": "code2006",
        "final_code_column_name": "code2012"
      }
    },
    {
      "check_ident": "qc_tool.vector.mmw_ua",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_name": "code2012",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "qc_tool.vector.mmw_ua",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_column_name": "code2006",
        "mmw": 10.0
      }
    },
    {
      "check_ident": "qc_tool.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change"]
      }
    },
    {
      "check_ident": "qc_tool.vector.neighbour",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "code_column_names": ["code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "qc_tool.vector.neighbour",
      "required": false,
      "parameters": {
        "layers": ["revised"],
        "code_column_names": ["code2006"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "qc_tool.vector.neighbour",
      "required": false,
      "parameters": {
        "layers": ["combined", "change"],
        "code_column_names": ["code2006", "code2012"],
        "exclude_codes": ["122%"]
      }
    },
    {
      "check_ident": "qc_tool.vector.inspire",
      "required": false,
      "parameters": {
        "layers": ["reference", "combined", "revised", "change", "boundary"]
      }
    }
  ]
}
