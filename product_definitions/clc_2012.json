    {
      "check_ident": "initial.v14",
      "required": false,
      "parameters": {
        "layers": ["initial"],
        "code_column_names": ["code_06"]
      }
    },
    {
      "check_ident": "change.v14",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "code_column_names": ["code_06", "code_12"]
      }
    },
    {
      "check_ident": "v15",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial", "change"]
      }
    }
  ]
