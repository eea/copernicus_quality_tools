{
  "description": "CORINE Land Cover 2012",
  "checks": [
    {
      "check_ident": "v_unzip",
      "required": true
    },
    {
      "check_ident": "v1_clc",
      "required": true,
      "parameters": {
        "reference_year": "2012",
        "gdb_filename_regex": "^clc2012_(?P<country_code>.+).gdb$",
        "reference_layer_regex": "^{country_code:s}/clc12_{country_code:s}$",
        "initial_layer_regex": "^{country_code:s}/clc06_{country_code:s}$",
        "change_layer_regex": "^{country_code:s}/cha12_{country_code:s}$"
      }
    },
    {
      "check_ident": "v2",
      "required": true,
      "parameters": {
        "layers": ["reference", "initial", "change"],
        "formats": [".gdb"]
      }
    },
    {
      "check_ident": "reference.v3",
      "required": true,
      "parameters": {
        "layers": ["reference"],
        "attributes": {"id": "string",
                       "remark": "string",
                       "area_ha": "real",
                       "shape_area": "real",
                       "shape_length": "real",
                       "code_12": "string"}
      }
    },
    {
      "check_ident": "initial.v3",
      "required": true,
      "parameters": {
        "layers": ["initial"],
        "attributes": {"id": "string",
                       "remark": "string",
                       "area_ha": "real",
                       "shape_area": "real",
                       "shape_length": "real",
                       "code_06": "string"}
      }
    },
    {
      "check_ident": "change.v3",
      "required": true,
      "parameters": {
        "layers": ["change"],
        "attributes": {"id": "string",
                       "remark": "string",
                       "area_ha": "real",
                       "shape_area": "real",
                       "shape_length": "real",
                       "change": "string",
                       "code_06": "string",
                       "code_12": "string",
                       "chtype": "string"}
      }
    },
    {
      "check_ident": "v4_clc",
      "required": true,
      "parameters": {
        "layers": ["reference", "initial", "change"]
      }
    },
    {
      "check_ident": "v_import2pg",
      "required": true
    },
    {
      "check_ident": "v5",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial", "change"],
        "unique_keys": ["id"]
      }
    },
    {
      "check_ident": "reference.v6",
      "required": false,
      "parameters": {
        "layers": ["reference"],
        "column_defs": [["code_12", "CLC"]]
      }
    },
    {
      "check_ident": "initial.v6",
      "required": false,
      "parameters": {
        "layers": ["initial"],
        "column_defs": [["code_06", "CLC"]]
      }
    },
    {
      "check_ident": "change.v6",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "column_defs": [["code_06", "CLC"], ["code_12", "CLC"]]
      }
    },
    {
      "check_ident": "v8",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial", "change"]
      }
    },
    {
      "check_ident": "v10",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial"]
      }
    },
    {
      "check_ident": "v11_clc_status",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial"],
        "area_m2": 250000,
        "area_column_name": "shape_area"
      }
    },
    {
      "check_ident": "v11_clc_change",
      "required": false,
      "parameters": {
        "layers": ["change"],
        "area_m2": 50000,
        "area_column_name": "shape_area",
        "initial_code_column_name": "code_06",
        "final_code_column_name": "code_12"
      }
    },
    {
      "check_ident": "v12",
      "required": false,
      "parameters": {
        "layers": ["reference", "initial", "change"],
